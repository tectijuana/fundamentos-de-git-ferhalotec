#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int numero, producto = 1;
    for (int i = 1; i <= 2; i++)
    {
        cout << "Numero " << i << endl;
        cin >> numero;

        producto *= numero;
    }

    cout << "La media geometrica es: " << sqrt(producto) << endl;

    return 0;
}